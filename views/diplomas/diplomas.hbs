<div class="container">
	<div class="header">
		<div class="row">
			<div class="col-lg-9">
				<h1>Diplomas</h1>
			</div>
			<div class="col-lg-3 text-right">
				<form method="get" action="/diplomas/register">
					<input id="button-challenge" type="submit" value="Nueva Diploma" class="btn btn-primary btn-lg"/>
				</form>
			</div>
		</div>
		<hr />
	</div>
	{{#each diplomas}}
	<div class="row well">
		<div class="col-lg-12">
			<h3 class="text-uppercase">{{diplomaName}}</h3>
			<h5>{{description}}</h5>
		</div>
	</div>
	{{/each}}
</div>
